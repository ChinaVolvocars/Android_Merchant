<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f0f0f0"
    android:orientation="vertical">

    <include layout="@layout/layout_toolbar_base" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@color/color_fefcfc">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rl_pay_for_ohter"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/view_point01"
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/circle_color_yellow_99fcc80a" />

                <TextView
                    android:id="@+id/view_point02"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/circle_color_yellow_fcc80a" />

                <TextView
                    android:id="@+id/tv_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="14dp"
                    android:text="代收代付"
                    android:textColor="@color/color_1e1e1e"
                    android:textSize="19sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/view_point03"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="14dp"
                    android:background="@drawable/circle_color_yellow_fcc80a" />

                <TextView
                    android:id="@+id/view_point04"
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/circle_color_yellow_99fcc80a" />
            </LinearLayout>

            <TextView
                android:id="@+id/view_bottomline_pay_for_ohter"
                android:layout_width="148dp"
                android:layout_height="2dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="6dp"
                android:background="@color/color_1e1e1e" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rl_invoice_withdraw"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/view_point05"
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:background="@drawable/circle_color_blue_9955a7fb" />

                <TextView
                    android:id="@+id/view_point06"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/circle_color_blue_55a7fb" />

                <TextView
                    android:id="@+id/tv_"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="14dp"
                    android:text="发票提现"
                    android:textColor="@color/color_1e1e1e"
                    android:textSize="19sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/view_point07"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="14dp"
                    android:background="@drawable/circle_color_blue_55a7fb" />

                <TextView
                    android:id="@+id/view_point08"
                    android:layout_width="4dp"
                    android:layout_height="4dp"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/circle_color_blue_9955a7fb" />

            </LinearLayout>

            <TextView
                android:id="@+id/view_bottomline_withdraw"
                android:layout_width="148dp"
                android:layout_height="2dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/color_1e1e1e" />
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/fragment_payfor_other" />



</LinearLayout>